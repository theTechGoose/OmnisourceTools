{
  "name": "OmnisourceTools",
  "version": "0.0.1",
  "description": "A collection of tools for Omnisource projects.",
  "compilerOptions": {
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@^1.0.14",
    "@cliffy/prompt": "jsr:@cliffy/prompt@^1.0.0-rc.7",
    "class-transformer": "npm:class-transformer@^0.5.1",
    "class-validator": "npm:class-validator@^0.14.2",
    "reflect-metadata": "npm:reflect-metadata@^0.2.2"
  },
  "tasks": {
    "deploy": "deno run -A deploy.ts"
  }
}
